Program.Sub.loaddtdvKA.Start
F.Intrinsic.Control.Try
	
	v.Local.iProgress.Declare
	v.Local.iCnt.Declare
	v.Local.sJob.Declare
	v.Local.sSfx.Declare
	v.Local.iFor.Declare
	v.Local.iOne.Declare
	v.Local.sMaterial.Declare
	v.Local.iCount.Declare
	v.Local.iMult.Declare
	V.Local.sSQL.Declare
	v.Local.sret.declare
	v.Local.sFilter.Declare
	v.Local.dStart.Declare
	v.Local.dEnd.Declare
	v.Local.sDateDueRange.Declare
	
	v.Local.dStart.Set(v.Screen.FrmKA!dtpStart.Value)
	v.Local.dEnd.Set(v.Screen.FrmKA!dtpEnd.Value)
	
	f.Intrinsic.Control.If(v.Screen.FrmKA!chkCutOff.Value,=,1)
		f.Intrinsic.Control.If(v.Local.dStart,>,v.Local.dEnd)
			f.Intrinsic.UI.Msgbox("Invalid date range.","Attention")
			f.Intrinsic.Control.ExitSub
		f.Intrinsic.Control.EndIf
		f.Intrinsic.String.Build(" and (Date_Due between '{0}' and '{1}') ",v.Local.dStart.PervasiveDate,v.Local.dEnd.PervasiveDate,v.Local.sDateDueRange)
	f.Intrinsic.Control.EndIf
	
	
	F.Intrinsic.Control.If(V.DataTable.dtKA.Exists)
		F.Data.DataTable.Close("dtKA")
	F.Intrinsic.Control.EndIf
	
	F.Intrinsic.String.Build("SELECT DISTINCT CASE WHEN  FLAG_WO_PRTD = 'R' OR FLAG_WO_PRTD = 'Y' THEN CONVERT(1,SQL_BIT) ELSE CONVERT(0,SQL_BIT) END AS FLAG_WO_PRTD,rtrim(C.NAME_CUSTOMER) as Name_Customer,A.JOB,A.SUFFIX,rtrim(CUSTOMER_PO) as Customer_PO,rtrim(PART) as Part,rtrim(DESCRIPTION) as Description,A.PRODUCT_LINE,rtrim(a.Description) as RTR_DESC,'+/- Seq' as AddRemoveSeq, PRIORITY,Case When IsNull(Flag_Hold,'') = 'Y' Then Convert(1,SQL_Bit) Else Convert(0,SQL_Bit) End  as OnHold,a.Salesperson,(SELECT TOP 1 VOH.DATE_ORDER FROM V_ORDER_TO_WO VOW LEFT JOIN V_ORDER_HEADER VOH ON VOH.ORDER_NO = VOW.ORDER_NO WHERE VOW.JOB = A.JOB AND VOW.SUFFIX = A.SUFFIX) AS PODT,(A.QTY_ORDER - A.QTY_COMPLETED) AS QTY_LEFT,DATE_DUE,'' AS {0}Col1{0},'' AS {0}Col2{0},'' AS {0}Col3{0},'' AS {0}Col4{0},'' AS {0}Col5{0},'' AS {0}Col6{0},'' AS {0}Col7{0},'' AS {0}Col8{0},'' AS {0}Col9{0},'' AS {0}Col10{0},'' AS seqRes,'' AS NOTES,rtrim(B.USER_1) as User_1,rtrim(B.USER_2) as User_2,rtrim(B.USER_3) as User_3,rtrim(B.USER_4) as User_4,rtrim(B.USER_5) as User_5,rtrim(B.USER_6) as User_6,rtrim(B.USER_7) as User_7,rtrim(B.USER_8) as User_8,rtrim(B.USER_9) as User_9,rtrim(B.USER_10) as User_10,LOCATION,PARENT_WO,PARENT_SUFFIX_PARENT,CONCAT(A.JOB,A.SUFFIX) AS JBSF,Case When a.DATE_ORIG_DUE = '1900-01-01' Then Null Else a.Date_Orig_Due End as Date_Orig_Due FROM V_JOB_HEADER A LEFT JOIN V_WO_USER_FLDS B ON A.JOB = B.JOB AND A.SUFFIX = B.SUFFIX LEFT JOIN V_CUSTOMER_MASTER C ON A.CUSTOMER = C.CUSTOMER WHERE DATE_CLOSED = '1900-1-1' {1};",V.Ambient.DblQuote,v.Local.sDateDueRange,V.Local.sSQL)
	F.Data.DataTable.CreateFromSQL("dtKA","con",V.Local.sSQL,true)	
	
	f.Intrinsic.Control.If(v.DataTable.dtKA.RowCount,=,0)
		f.Intrinsic.UI.Msgbox("No Record found.","Attention")
		gui.FrmKA.GsGCKA.AddGridviewFromDatatable("gvKA","dtKA")
		gui.FrmKA.GsGCKA.MainView("gvKA")
		f.Intrinsic.Control.ExitSub
	f.Intrinsic.Control.EndIf
	'Add Note
	F.Data.Dictionary.CreateFromSQL("dNotes","con","SELECT CONCAT(JOB,SUFFIX) AS JBSF, RTRIM(NOTES) AS NOTES FROM GAB_4169_WO_NOTES;")
	F.Data.Dictionary.SetDefaultReturn("dNotes","")
	F.Data.DataTable.FillFromDictionary("dtKA","dNotes","JBSF","NOTES")
	F.Data.Dictionary.Close("dNotes")
	
	Gui.FrmKA.lblStatus.Visible(True)
	Gui.FrmKA.pbStatus.Visible(True)
	Gui.FrmKA.lblStatus.Caption("Loading Work Orders....")
	Gui.FrmKA.pbStatus.Min(0)
	Gui.FrmKA.pbStatus.Max(V.DataTable.dtKA.RowCount)

	f.Intrinsic.Control.If(v.DataTable.Rem.Exists)
